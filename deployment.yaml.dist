apiVersion: apps/v1
kind: Deployment
metadata:
  name: my-go-app
spec:
  selector:
    matchLabels:
      app: go-app
  template:
    metadata:
      labels:
        app: go-app
    spec:
      containers:
      - name: go-app-container
        image: eu.gcr.io/tao-dev-281909/cam-dev:latest
        resources:
          limits:
            memory: "128Mi"
            cpu: "500m"
        ports:
        - containerPort: 3000


